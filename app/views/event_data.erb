<%= partial :'_sidebar' %>
<div class="ten columns">
  <%= partial :'_header' %>
  <main>
    <div class="row">
      <div class="twelve column box">
        <h2>Ordered Events</h2>
        <ul class="events">
          <% @source.ordered_events.each do |event| %>
          <li><a href="/sources/<%= @source.identifier %>/events/<%= event %>"><%= event  %></a></li>
          <% end %>
        </ul>
      </div><!--column-->
    </div><!--row-->
  </main>
</div>
